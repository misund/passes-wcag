{
  "name": "passes-wcag",
  "version": "0.2.1",
  "description": "Evaluate if a to colors have sufficient contrast to pass WCAG requirements.",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "lint": "eslint --max-warnings=0 src",
    "mocha": "mocha --require @babel/register",
    "prepublish": "yarn build && yarn test",
    "postpublish": "git push --tags",
    "release": "np",
    "test": "yarn lint && yarn mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misund/passes-wcag.git"
  },
  "keywords": [
    "wcag",
    "color",
    "contrast",
    "luminance",
    "contrast-ratio",
    "accessibility",
    "a11y",
    "relative-luminance",
    "color-contrast",
    "text-legibility"
  ],
  "author": "Just Thomas Hiorth Misund",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/misund/passes-wcag/issues"
  },
  "homepage": "https://github.com/misund/passes-wcag#readme",
  "devDependencies": {
    "@babel/cli": "7.28.0",
    "@babel/core": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@babel/register": "7.27.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "mocha": "11.7.1",
    "np": "10.2.0"
  },
  "dependencies": {
    "get-contrast-ratio": "0.2.1"
  }
}
